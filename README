vagrant plugin install vagrant-vbguest

sudo systemd-run \
    --unit=caddy \
    rkt run \
    --dns=8.8.8.8 \
    --hostname=caddy.rktnet \
    --net=bridgenet:IP=10.2.0.10 \
    --hosts-entry 10.2.0.10=caddy.rktnet \
    --hosts-entry 10.2.0.11=inspector.rktnet \
    --volume=www,kind=host,source=/home/vagrant/src/www --mount=volume=www,target=/var/www/html \
    --insecure-options=image \
    docker://joshix/caddy

sudo systemd-run \
    --unit=inspector \
    rkt run \
    --dns=8.8.8.8 \
    --hostname=inspector.rktnet \
    --net=bridgenet:IP=10.2.0.11 \
    --hosts-entry 10.2.0.10=caddy.rktnet \
    --hosts-entry 10.2.0.11=inspector.rktnet \
    s-urbaniak.github.io/images/inspector:0.0.2

